<div class="page-header">
  <h1>
    Involvements
    <a href="/involvements/new" class="btn btn-lg btn-success">New Involvement</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Industry</th>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @involvements.each do |involvement| %>
      <tr>
        <td><%= involvement.industry_id %></td>
        <td><%= involvement.user_id %></td>
        <td>
          <a href="/involvements/<%= involvement.id %>" class="btn btn-primary">Show</a>
          <a href="/involvements/<%= involvement.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_involvement/<%= involvement.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
