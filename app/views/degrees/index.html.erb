<div class="page-header">
  <h1>
    Degrees
    <a href="/degrees/new" class="btn btn-lg btn-success">New Degree</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>School</th>
        <th>User</th>
        <th>Degree</th>
        <th>Grad year</th>
        <th>Actions</th>
      </tr>

      <% @degrees.each do |degree| %>
      <tr>
        <td><%= degree.school_id %></td>
        <td><%= degree.user_id %></td>
        <td><%= degree.degree %></td>
        <td><%= degree.grad_year %></td>
        <td>
          <a href="/degrees/<%= degree.id %>" class="btn btn-primary">Show</a>
          <a href="/degrees/<%= degree.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_degree/<%= degree.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
